<?xml version="1.0" encoding="UTF-8"?>
<qti-assessment-test
    xmlns="http://www.imsglobal.org/xsd/imsqtiasi_v3p0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqtiasi_v3p0
                        https://purl.imsglobal.org/spec/qti/v3p0/schema/xsd/imsqti_asiv3p0_v1p0.xsd"
    identifier="katex-test"
    title="KaTeX Test"
    toolName="QtiLearningApp"
    toolVersion="1.0">

    <qti-test-part identifier="part1" navigation-mode="linear" submission-mode="individual">
        <qti-assessment-section identifier="section1" title="KaTeX Test" visible="true">
            <qti-assessment-item-ref identifier="qgleichung" href="../questions/katex_q1.xml"/>
            <qti-assessment-item-ref identifier="ableitung" href="../questions/katex_q2.xml"/>
            <qti-assessment-item-ref identifier="pythagoras" href="../questions/katex_q3.xml"/>
            <qti-assessment-item-ref identifier="vereinfachen" href="../questions/katex_q4.xml"/>
            <qti-assessment-item-ref identifier="rendertest" href="../questions/katex_q5.xml"/>
        </qti-assessment-section>
    </qti-test-part>

    <qti-outcome-declaration identifier="TOTAL_SCORE" cardinality="single" base-type="float">
        <qti-default-value>
            <qti-value>0</qti-value>
        </qti-default-value>
    </qti-outcome-declaration>

    <qti-outcome-processing>
        <qti-set-outcome-value identifier="TOTAL_SCORE">
            <qti-sum>
                <qti-outcome-variable identifier="SCORE"/>
            </qti-sum>
        </qti-set-outcome-value>
    </qti-outcome-processing>

</qti-assessment-test>
