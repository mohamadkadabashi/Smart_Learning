<?xml version="1.0" encoding="UTF-8"?>
<qti-assessment-item
    xmlns="http://www.imsglobal.org/xsd/imsqtiasi_v3p0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqtiasi_v3p0
                        https://purl.imsglobal.org/spec/qti/v3p0/schema/xsd/imsqti_asiv3p0_v1p0.xsd"
    identifier="sql_erstellen"
    title="SQL Tabelle erstellen"
    adaptive="false"
    time-dependent="false"
    xml:lang="de">

    <qti-response-declaration
        identifier="RESPONSE"
        cardinality="single"
        base-type="string">
        <qti-correct-response>
            <qti-value>CREATE</qti-value>
        </qti-correct-response>
    </qti-response-declaration>

    <qti-outcome-declaration
        identifier="SCORE"
        cardinality="single"
        base-type="float">
        <qti-default-value>
            <qti-value>1.0</qti-value>
        </qti-default-value>
    </qti-outcome-declaration>

    <qti-item-body>
        <qti-prompt>Mit welchem Befehl kann man in SQL eine neue Tabelle erstellen?</qti-prompt>
        <qti-text-entry-interaction
            response-identifier="RESPONSE"
            expected-length="10"/>
    </qti-item-body>

    <qti-response-processing template="https://purl.imsglobal.org/spec/qti/v3p0/rptemplates/match_correct.xml"/>
</qti-assessment-item>
